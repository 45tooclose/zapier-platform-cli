{
  "name": "@zapier/zapier-platform-cli",
  "version": "3.2.6",
  "description": "The CLI for the Zapier platform.",
  "repository": "zapier/zapier-platform-cli",
  "homepage": "https://zapier.com/",
  "author": "Bryan Helmig <bryan@zapier.com>",
  "license": "BSD",
  "main": "zapier.js",
  "engines": {
    "node": ">=v4.3.2"
  },
  "scripts": {
    "docs": "node lib/bin/docs.js",
    "preversion": "git pull && npm test",
    "version": "npm run docs && git add docs/*",
    "postversion": "git push && git push --tags && npm publish",
    "prepublish": "npm run build",
    "build": "rm -rf lib && node_modules/babel-cli/bin/babel.js src -d lib",
    "watch": "rm -rf lib && node_modules/babel-cli/bin/babel.js --watch src -d lib",
    "lint": "node_modules/.bin/eslint zapier.js src test",
    "test": "npm run build && node_modules/mocha/bin/mocha -t 10000 --recursive lib/tests && npm run lint",
    "zapier": "zapier.js"
  },
  "dependencies": {
    "adm-zip": "0.4.7",
    "babel-polyfill": "6.9.1",
    "browserify": "13.0.1",
    "cli-table2": "0.2.0",
    "colors": "1.1.2",
    "fs-extra": "0.30.0",
    "lodash": "4.13.1",
    "node-fetch": "1.5.3",
    "string-length": "^1.0.1",
    "through2": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "6.9.0",
    "eslint": "^3.2.2",
    "fetch-mock": "4.6.0",
    "litdoc": "1.5.2",
    "mocha": "2.5.3",
    "should": "9.0.2"
  },
  "bin": {
    "zapier": "zapier.js"
  }
}
