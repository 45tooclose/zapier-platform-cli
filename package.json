{
  "name": "@zapier/zapier-platform-cli",
  "version": "3.2.4",
  "description": "The CLI for the Zapier platform.",
  "repository": "zapier/zapier-platform-cli",
  "homepage": "https://zapier.com/",
  "author": "Bryan Helmig <bryan@zapier.com>",
  "license": "BSD",
  "main": "zapier.js",
  "scripts": {
    "docs": "node lib/bin/docs.js",
    "preversion": "git pull && npm test",
    "version": "npm run docs && git add docs/*",
    "postversion": "git push && git push --tags && npm publish",
    "prepublish": "npm run build",
    "build": "rm -rf lib && node_modules/babel-cli/bin/babel.js src -d lib",
    "watch": "rm -rf lib && node_modules/babel-cli/bin/babel.js --watch src -d lib",
    "test": "node_modules/mocha/bin/mocha lib/tests",
    "zapier": "zapier.js"
  },
  "dependencies": {
    "adm-zip": "0.4.7",
    "babel-polyfill": "6.9.1",
    "browserify": "13.0.1",
    "cli-table2": "0.2.0",
    "colors": "^1.1.2",
    "fs-extra": "0.30.0",
    "lodash": "4.13.1",
    "node-fetch": "1.5.3",
    "through2": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.10.1",
    "babel-preset-es2015": "6.9.0",
    "fetch-mock": "4.6.0",
    "highlight.js": "^9.5.0",
    "markdown-toc": "^0.12.15",
    "marked": "0.3.6",
    "mocha": "2.5.3",
    "should": "9.0.2"
  },
  "bin": {
    "zapier": "zapier.js"
  }
}
